<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.itcast.zxxx.entity.Course" table="course">
	    <id name="id1" type="java.lang.String" >
	    	<column name="id" />
	    	<generator class="uuid.hex"/>
	    </id>
		<property name="name1" type="java.lang.String">
			<column name="name" length="255" not-null="true" />
		</property>
		<property name="content1" type="java.lang.String">
			<column name="content" length="255" not-null="true"/>
		</property>
	</class>
</hibernate-mapping>